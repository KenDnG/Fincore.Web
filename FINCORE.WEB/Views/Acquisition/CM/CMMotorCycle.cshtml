                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                            <input type="hidden" name="bank_account_id_umc" id="hdBankAccountID" />
                                            <input type="hidden" name="bank_id_umc" id="txBankNameId" />
                                            <textarea class="form-control" id="txBankName" placeholder="Pilih Nama Bank" name="bank_name_umc" required></textarea>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                            <button id="btnshow_BankName" class="btn-input" type="button" onclick="cm_lookup.LookupType('BankName', this.id)" style="width: 100%; margin-bottom: 5%;"><i id="ibtnshow_BankName" class="fa fa-sign-in fa-lg fa-fw"></i>Pilih Nama Bank</button>

                                            <div id="mdlBankName" class="modal">
