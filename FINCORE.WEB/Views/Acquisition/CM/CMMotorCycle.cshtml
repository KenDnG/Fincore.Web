                                            var AccountOwner = ViewBag.AccountOwner;

                                            for (int i = 0; i < @AccountOwner.Count; i++)
                                            {
                                                <option value="@AccountOwner[i].account_owner_id">@AccountOwner[i].account_owner_desc</option>
                                            }
                                        }
                                    </select>

