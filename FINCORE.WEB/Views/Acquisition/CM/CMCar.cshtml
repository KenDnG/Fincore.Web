                                            <input type="hidden" id="txhiddenCGSCabangNo" value="@CMCarModels[i].CGSCabangNo" />
                                            <input type="hidden" id="txhiddendisc_type" value="@CMCarModels[i].disc_type" />
                                            <input type="hidden" id="txhiddenTAC_max" value="@CMCarModels[i].TAC_max" />
                                            <input type="hidden" id="txhiddenkomper_max" value="@CMCarModels[i].komper_max" />
                                            <input type="hidden" id="txhiddenis_avalis" value="@CMCarModels[i].is_avalis" />
                                            <input type="hidden" id="txhiddeninstallment_type" value="@CMCarModels[i].installment_type" />
