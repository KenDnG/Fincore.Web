@model FINCORE.Models.Models.Acquisition.CM.CMCarModels
@using FINCORE.Models.Models.Acquisition.CM
@using FINCORE.Services.Helpers.Response;
@{
    ViewData["Title"] = "AKUISISI MOBIL DATA NASABAH - CM";
}
<br />

    <form id="contactform" asp-action="SaveCM" asp-controller="CMCar" method="post" enctype="multipart/form-data">

        <input type="hidden" id="txhiddenFlag" value="@Context.Session.GetString("Flag")" />
        <input type="hidden" id="txhiddenCompanyId" name="company_id" value="@Context.Session.GetString("CompanyId")" />
        <input type="hidden" id="txhiddenBranchId" name="branch_id" value="@Context.Session.GetString("BranchId")" />

        <input type="hidden" ID="hdnInterestMin" />
        <input type="hidden" ID="hdnInterestMax" />
        <input type="hidden" ID="hdnRefundBunga" />
        <input type="hidden" ID="hdnInsRefundMax" />
        <input type="hidden" ID="hdnAdminFeeMin" />
        <input type="hidden" ID="hdnAdminFeeMax" />
        <input type="hidden" ID="hdnBiayaProses" /> 
        <input type="hidden" ID="hdnNilaiBunga" />
        <input type="hidden" ID="hdnTACMax" />
        <input type="hidden" ID="hdnNominalRefundBunga" />
        <input type="hidden" ID="hdnSubsidiPihakKetigaMax" />   
    
    @{
        var TACMax = ViewBag.TACMax;
        for (int i = 0; i < @TACMax.Count; i++)
        {
            <input type="hidden" ID="hdnBatasTACMax" value="@TACMax[i].value" />
        }

        var ASURANSITACMAX = ViewBag.ASURANSITACMAX;
        for (int i = 0; i < @ASURANSITACMAX.Count; i++)
        {
            <input type="hidden" ID="hdnParamAsuransiTACMax" value="@ASURANSITACMAX[i].value" />
        }

        var BranchException = ViewBag.BranchException;
        for (int i = 0; i < @BranchException.Count; i++)
        {
            <input type="hidden" ID="hdnIsBranchException" value="@BranchException[i].branch_exception" />
        }
    } 


    <!--/ INPUT CM -->  
        <section class="feature-wrapper">
            <div class="containers" style="margin-top: -5%;">

                <!-- DATA ASSET -->
                <div class="container">
                    <button id="Asset_more" class="Asset_more" type="button" style="width: 100%;">Data Asset</button>
                    <btnAsset><h4><div class="bgc-menu">Data Asset <i class="fa fa-chevron-circle-down" aria-hidden="true"></i></div></h4></btnAsset>
                    <Asset class="Asset" id="Asset">
                        <div class="tile">
                            <div class="form-group row">

                                <!-- KOLOM 1 -->
                                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="border-right: 2px solid #e8e8e8">
                                    <p>Credit ID</p> 
                                    @if (Context.Session.GetString("Flag") == "Add")
                                    {
                                        <textarea class="form-control" readonly name="credit_id" id="txcredit_id" placeholder="Credit ID" required>@Context.Session.GetString("credit_id")</textarea>
                                    }
                                    else
