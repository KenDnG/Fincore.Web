                                            <input type="hidden" id="txhiddensurveyor_id" value="@CMCarModels[i].surveyor_id" />
                                            <input type="hidden" id="txhiddensurveyor_name" value="@CMCarModels[i].surveyor_name" />
                                            <input type="hidden" id="txhiddenmarketinghead_id" value="@CMCarModels[i].marketinghead_id" />
                                            <input type="hidden" id="txhiddenmarketinghead_name" value="@CMCarModels[i].marketinghead_name" />
                                            <input type="hidden" id="txhiddenCC" value="@CMCarModels[i].CC" />
                                            <input type="hidden" id="txhiddenar" value="@CMCarModels[i].ar" />
                                            <input type="hidden" id="txhiddentipe_cover" value="@CMCarModels[i].tipe_cover" />
                                            
                                            <input type="hidden" id="txhiddeninterest_rate_type_id" value="@CMCarModels[i].interest_rate_type_id" />
                                            <input type="hidden" id="txhiddentenor" value="@CMCarModels[i].tenor" />
                                            <input type="hidden" id="txhiddenasset_cost" value="@CMCarModels[i].asset_cost" />
                                            <input type="hidden" id="txhiddengross_down_payment" value="@CMCarModels[i].gross_down_payment" />

                                            <input type="hidden" id="txhiddenadmin_fee" value="@CMCarModels[i].admin_fee" />
                                            <input type="hidden" id="txhiddenadmin_fee_kredit" value="@CMCarModels[i].admin_fee_kredit" />

