                                            <input type="hidden" id="txhiddennominal_biaya_proses_kredit" value="@CMCarModels[i].nominal_biaya_proses_kredit" />

                                            <input type="hidden" id="txhiddenloss_fee" value="@CMCarModels[i].loss_fee" />
                                            <input type="hidden" id="txhiddenloss_fee_kredit" value="@CMCarModels[i].loss_fee_kredit" />

                                            <input type="hidden" id="txhiddenprovisi_ins_fee" value="@CMCarModels[i].provisi_ins_fee" />
                                            <input type="hidden" id="txhiddenprovisi_ins_fee_kredit" value="@CMCarModels[i].provisi_ins_fee_kredit" />

                                            <input type="hidden" id="txhiddencustomer_pay_amount" value="@CMCarModels[i].customer_pay_amount" />
                                            <input type="hidden" id="txhiddeninstallment_code" value="@CMCarModels[i].installment_code" />

                                            <input type="hidden" id="txhiddenresidual_value" value="@CMCarModels[i].residual_value" />

                                            <input type="hidden" id="txhiddendiscount_dealer" value="@CMCarModels[i].discount_dealer" />

                                            <input type="hidden" id="txhiddenongkos_BBN" value="@CMCarModels[i].ongkos_BBN" />
                                            <input type="hidden" id="txhiddenongkos_tagih" value="@CMCarModels[i].ongkos_tagih" /> 
                                            
                                            <input type="hidden" id="txhiddenSubsidiFinance" value="@CMCarModels[i].SubsidiFinance" /> 
                                            <input type="hidden" id="txhiddenSubsidiDealer" value="@CMCarModels[i].SubsidiDealer" /> 
                                            <input type="hidden" id="txhiddenSubsidiMainDealer" value="@CMCarModels[i].SubsidiMainDealer" /> 
                                            <input type="hidden" id="txhiddenSubsidiATPM" value="@CMCarModels[i].SubsidiATPM" /> 
                                            <input type="hidden" id="txhiddenSubsidiPihakKetiga" value="@CMCarModels[i].SubsidiPihakKetiga" /> 
                                            <input type="hidden" id="txhiddenSubsidiBunga" value="@CMCarModels[i].SubsidiBunga" /> 

                                            <input type="hidden" id="txhiddenmega_insurance_fee" value="@CMCarModels[i].mega_insurance_fee" /> 
                                            <input type="hidden" id="txhiddenmega_insurance_fee_kredit" value="@CMCarModels[i].mega_insurance_fee_kredit" /> 
                                            <input type="hidden" id="txhiddenhandphone_fee" value="@CMCarModels[i].handphone_fee" /> 
                                            <input type="hidden" id="txhiddenhandphone_fee_kredit" value="@CMCarModels[i].handphone_fee_kredit" />

                                            <input type="hidden" id="txhiddenis_life_ins" value="@CMCarModels[i].is_life_ins" />

                                            <input type="hidden" id="txhiddenskema_id" value="@CMCarModels[i].skema_id" />
                                            <input type="hidden" id="txhiddenperantara_type_id" value="@CMCarModels[i].perantara_type_id" />
                                            <input type="hidden" id="txhiddenagent_id" value="@CMCarModels[i].agent_id" />
                                            <input type="hidden" id="txhiddenagent_name" value="@CMCarModels[i].agent_name" />
                                            <input type="hidden" id="txhiddenownership_account_type_id" value="@CMCarModels[i].ownership_account_type_id" />
                                            <input type="hidden" id="txhiddenbank_account_id_umc" value="@CMCarModels[i].bank_account_id_umc" />
                                            <input type="hidden" id="txhiddenbank_id_umc" value="@CMCarModels[i].bank_id_umc" />
                                            <input type="hidden" id="txhiddenbank_name_umc" value="@CMCarModels[i].bank_name_umc" />
                                            <input type="hidden" id="txhiddenaccount_no_umc" value="@CMCarModels[i].account_no_umc" />
                                            <input type="hidden" id="txhiddenaccount_name_umc" value="@CMCarModels[i].account_name_umc" />

                                            i++;
                                        }
                                    }

                                    <p>Tipe Aplikasi</p>
                                    <select class="form-control" name="application_type_id" id="ms_application_type" required>
                                        <option value="">Pilih Tipe Aplikasi</option>
                                        @{
                                            var ms_application_type = ViewBag.ms_application_type;
                                            for (int i = 0; i < @ms_application_type.Count; i++)
                                            {
                                                    <option value="@ms_application_type[i].application_type_id">@ms_application_type[i].application_type_name</option>
                                            }
                                        }
                                    </select>
                                    <label id="lblNoteTipeAplikasi" style="color: Red;"></label>

                                    <p>Tipe Finance</p>
                                    <select class="form-control" name="product_finance_id" id="ms_product_finance" required>
                                        <option value="">Pilih Tipe Finance</option>
                                        @{
                                            var ms_product_finance = ViewBag.ms_product_finance;

                                            for (int i = 0; i < @ms_product_finance.Count; i++)
                                            {
                                                            <option value="@ms_product_finance[i].product_finance_id">@ms_product_finance[i].product_finance_name</option>
                                            }
                                        }
                                    </select>

                                    <p>Baru / Bekas</p>
                                    <select class="form-control" id="is_item_new" required>
                                        <option value="" selected>Pilih Baru / Bekas</option>
                                        <option value="1">Baru</option>
                                        <option value="0">Bekas</option>
                                    </select>
                                    <input type="hidden" id="hdis_item_newSelecytedValue" name="is_item_new" />

                                    <p>Item</p>
                                    <select class="form-control" id="asset_kind_id" name="asset_kind_id" required>
                                        <option value="">Pilih Item</option>
                                        @{
                                            var ms_asset_kind = ViewBag.ms_asset_kind;

                                            for (int i = 0; i < @ms_asset_kind.Count; i++)
                                            {
                                                            <option value="@ms_asset_kind[i].asset_kind_id">@ms_asset_kind[i].asset_kind_description</option>
                                            }
                                        }
                                    </select>

                                    <p>Merk Item</p>
                                    <select class="form-control" name="item_brand_id" id="ms_item_brand" required>
                                        <option value="">Pilih Merk Item</option>
                                    </select>

                                    <p style="margin-top: 2%;">Model Item</p>
                                    <select class="form-control" name="asset_kind_class_id" id="ms_asset_kind_class" required>
                                        <option value="">Pilih Model Item</option>
                                    </select>

                                    <p>Pilih Tipe Item</p>
                                    <div class="row">
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                            <input type="hidden" id="txitem_brand_type_id" name="item_brand_type_id" />
                                            <textarea class="form-control" readonly id="txItemname" name="item_type_name" placeholder="Pilih Tipe Item" required></textarea>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                            <button id="btnshow_Item" class="btn-input ladda-button" type="button" data-style="zoom-in" style="width: 100%; margin-bottom: 5%;">Pilih Tipe Item</button>

                                            <div id="mdlitem" class="modal">
                                                <!-- Modal content -->
                                                <div class="modal-content">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                <span aria-hidden="true">&times;</span>
                                                            </button>
                                                            <h4>Pilih Tipe Item</h4>
                                                            <div class="tile">
                                                                <div class="modal-body" id="containerItem"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>                                

                                    <p>CC</p>
                                    <input type="text" name="CC" id="CC" class="form-control" placeholder="CC" readonly>

                                    <p>Tahun Kendaraan</p>
                                    <select class="form-control" name="year" id="year" required>
                                        <option value="">Pilih Tahun Kendaraan</option>
                                    </select>

                                    <p>Produk</p>
                                    <select class="form-control" name="product_id" id="product_id" required>
                                        <option>Pilih Produk</option>
                                    </select>

                                </div>

                                <!-- KOLOM 2 -->
                                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                

                                    <p>Pilih Dealer</p>
                                    <div class="row">
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                            <input type="hidden" name="dealer_code" id="txdealercode" />
                                            <textarea class="form-control" readonly name="dealer_name" id="txdealername" placeholder="Pilih Dealer" required></textarea>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                            <button id="btnshow_Dealer" class="btn-input ladda-button" type="button" data-style="zoom-in" style="width: 100%; margin-bottom: 5%;">Pilih Dealer</button>

                                            <div id="mdldealer" class="modal">
                                                <!-- Modal content -->
                                                <div class="modal-content">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                <span aria-hidden="true">&times;</span>
                                                            </button>
                                                            <h4>Pilih Dealer</h4>
                                                            <div class="tile">
                                                                <div class="modal-body" id="containerDealer"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <p>Pilih Surveyor</p>
                                    <div class="row">
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                            <input type="hidden" name="surveyor_id" id="txSurveyorid" />
                                            <textarea class="form-control" readonly name="surveyor_name" id="txSurveyorname" placeholder="Pilih Surveyor" required></textarea>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                            <button id="btnshow_Surveyor" class="btn-input ladda-button" type="button" data-style="zoom-in" style="width: 100%; margin-bottom: 5%;">Pilih Surveyor</button>

                                            <div id="mdlsurveyor" class="modal">
