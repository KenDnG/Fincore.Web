                                            <input type="hidden" id="txhiddenjml_pembiayaan" value="@CMCarModels[i].jml_pembiayaan" />
                                            <input type="hidden" id="txhiddenamount_installment" value="@CMCarModels[i].amount_installment" />
                                            <input type="hidden" id="txhiddeneffective_rate" value="@CMCarModels[i].effective_rate" />
                                            <input type="hidden" id="txhiddenflat_rate" value="@CMCarModels[i].flat_rate" />
                                            <input type="hidden" id="txhiddendisc_deposit" value="@CMCarModels[i].disc_deposit" />
