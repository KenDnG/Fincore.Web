                                            <input type="hidden" id="txhiddenusage_type_name" value="@CMCarModels[i].usage_type_name" />
                                            <input type="hidden" id="txhiddenitem_brand_type_id" value="@CMCarModels[i].item_brand_type_id" />
                                            <input type="hidden" id="txhiddenitem_type_name" value="@CMCarModels[i].item_type_name" />
