                                            <input type="hidden" id="txhiddenyear" value="@CMCarModels[i].year" />
                                            <input type="hidden" id="txhiddenproduct_id" value="@CMCarModels[i].product_id" />
                                            <input type="hidden" id="txhiddenproduct_name" value="@CMCarModels[i].product_name" />
                                            <input type="hidden" id="txhiddenproduct_marketing_id" value="@CMCarModels[i].product_marketing_id" />
                                            <input type="hidden" id="txhiddenproduct_marketing_name" value="@CMCarModels[i].product_marketing_name" />
                                            <input type="hidden" id="txhiddenSTNK_name_id" value="@CMCarModels[i].STNK_name_id" />
                                            <input type="hidden" id="txhiddenSTNK_name_description" value="@CMCarModels[i].STNK_name_description" />
                                            <input type="hidden" id="txhiddenprovenance_PO_id" value="@CMCarModels[i].provenance_PO_id" />
                                            <input type="hidden" id="txhiddenprovenance_PO_description" value="@CMCarModels[i].provenance_PO_description" />
