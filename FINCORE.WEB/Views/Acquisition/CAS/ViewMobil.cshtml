@using FINCORE.Models.Models.Acquisition.CAS
@using FINCORE.Models.Models.Acquisition.Masters
@using FINCORE.Models.Models.Masters
@using FINCORE.Services.Helpers.Response
@using FINCORE.WEB.Helpers
@using static FINCORE.Services.Helpers.Response.Collection
@inject IHttpContextAccessor HttpContextAccessor

@model TrCasModels
@{
    ViewData["Title"] = "Akuisisi Mobil Data Nasabah - CAS";
}

@{
    var dta = Model;
    var isEdit = Sessions.GetSessionFromJson<Boolean>(HttpContextAccessor.HttpContext.Session, SessionIdentity.ISEDIT_KEY_NAME);
}

<div class="container">
    <form id="contactform" method="post">
        <!-- CAS HEADER -->
        <div class="container">
            <button id="CASheader_more" class="CASheader_more" type="button" style="width: 100%;">CAS Header</button>
            <div class="data-input">
                <btnCAS><h4><div class="bgc-menu">CAS Header <i class="fa fa-chevron-circle-down" aria-hidden="true"></i></div></h4></btnCAS>
                <CASheader class="CASheader" id="CASheader">
                    <div class="tile">
                        <div class="form-group row">

                            <!-- KOLOM 1 -->
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="border-right: 2px solid #e8e8e8">

                                <p>CAS No</p>
                                @if (isEdit)
                                {
                                    <input id="txtcredit_id" type="text" readonly="readonly" asp-for="credit_id" class="form-control" name="credit_id">
                                }
                                else
                                {
                                    <input id="txtcredit_id" type="text" readonly="readonly" asp-for="credit_id" class="form-control" name="credit_id" placeholder=" Generated By System ">
                                }

                                <p>Sumber CAS</p>
                                <select class="form-control" id="ddlcreditsource" onchange="cas_load.CheckOrderIdByCreditSource()" asp-for="credit_source_id" asp-items="@ViewBag.CASSource" required>
                                    <option value="">Pilih Sumber CAS</option>
                                </select>

                            </div>
                            <!-- END KOLOM 1 -->
                            <!-- KOLOM 2 -->
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">

                                <p>Order ID </p>
                                <div class="row">
                                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                        <input type="text" id="txtorderId" class="form-control" readonly="readonly" asp-for="order_id" placeholder="Pilih Order ID">
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        <button class="btn btn-warning btn-input ladda-button" type="button" id="btnshoworderid" data-style="zoom-in" style="width: 100%; margin-bottom: 5%;" disabled>Pilih Order ID</button>
                                        <!-- The Modal -->
                                        <div id="mdlorder" class="modal">
                                            <!-- Modal content -->
                                            <div class="modal-content">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                        <h4>Pilih Order ID</h4>
                                                        <div class="tile">
                                                            <div class="modal-body" id="containerOrder"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <p style="margin-top: 1%;">Tipe Konsumen</p>
                                <select class="form-control" id="ddltipekonsumen" asp-for="customer_type" onchange="cas_load.CheckTipeKonsumen();" asp-items="@ViewBag.CustomerType" required>
                                    <option value="">Pilih Tipe Customer</option>
                                </select>

                                <p style="margin-top: 8%;">Existing Customer (RO)</p>

                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p><input class="form-check-input" type="radio" value="true" asp-for="is_repeat_order" onclick="cas_load.CustomerROCheck();" id="ROyesCheck">&nbsp;&nbsp;&nbsp;&nbsp;Ya</p>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p><input class="form-check-input" type="radio" value="false" asp-for="is_repeat_order" onclick="cas_load.CustomerROCheck();" id="noCheck">&nbsp;&nbsp;&nbsp;&nbsp;Tidak</p>
                                </div>

                            </div>
                            <!-- END KOLOM 2 -->

                        </div>
                    </div>
                </CASheader>
            </div>
        </div>
        <!-- END CAS HEADER -->
        <!-- PERORANGAN -->
        <div class="container">
            <button id="Identitas_more" class="Identitas_more" type="button" style="width: 100%;">Identitas</button>
            <div class="data-input">
                <btnIdentitas><h4><div class="bgc-menu">Identitas <i class="fa fa-chevron-circle-down" aria-hidden="true"></i></div></h4></btnIdentitas>
                <Identitas class="Identitas" id="Identitas">
                    <div class="tile">
                        <div class="form-group row">

                            <!-- KOLOM 1 -->
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="border-right: 2px solid #e8e8e8">

                                <p>No KTP</p>
                                <div class="row">
                                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                        <input type="number" id="txKTP" asp-for="identity_number" maxlength="16" pattern="/^-?\d+\.?\d*$/" onKeyPress="if(this.value.length==16) return false;" class="form-control" placeholder="Input No KTP" required>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        <button class="btn btn-warning btn-input" id="btncheckBL_identity" value="@Collection.ItemId.MOBIL_CODE" style="width: 100%; margin-bottom: 5%;">Cek blacklist</button>
                                    </div>
                                    <p id="lblblacklist" style="display:none;"></p>
                                    <input type="hidden" asp-for="is_blacklist" id="txt_hidden_blacklist" />
                                </div>

                                <div class="row">
                                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                        <p>Berlaku Hingga : </p>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        <p><input type="checkbox" id="chkSeumurHidup"> Seumur Hidup</p>
                                    </div>
                                </div>
                                <input class="form-control" id="tanggalberlakuktp" asp-for="valid_thru" type="text" placeholder="Input Tanggal Berlaku KTP" required>

                                <p>Issue Date</p>
                                <input class="form-control" id="issuedatektp" asp-for="issue_date" type="text" placeholder="Input Issue Date KTP" required>

                                <p>Alamat KTP</p>
                                <textarea class="form-control" asp-for="identity_address" id="txidentityaddress" rows="" placeholder="Input Alamat KTP" required></textarea>

                                <p>Lokasi KTP</p>
                                <div class="row">
                                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                        <input type="hidden" asp-for="identity_location_id" id="txlocationktpid" required />
                                        @{
                                            var locationKtpName = "";
                                            if (isEdit)
                                            {
                                                var dataLocation = (MsLocationDetailModels)Commons.GetLoctionById((int)Model.identity_location_id);
                                                if (dataLocation != null)
                                                {
                                                    locationKtpName = Commons.CombineLocationName(dataLocation.village_name, dataLocation.district_name, dataLocation.regency_name, dataLocation.province_name, dataLocation.zip_code);
                                                }
                                            }
                                        }
                                        <textarea class="form-control" id="txlocationktpname" disabled="" rows="" placeholder="Pilih Lokasi KTP" required>@locationKtpName</textarea>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        <button class="btn btn-warning btn-input ladda-button" type="button" id="btnshowlokasiktp" data-style="zoom-in" style="width: 100%; margin-bottom: 5%;">Pilih lokasi KTP</button>
                                    </div>
                                </div>

                                <p>Alamat</p>
                                <textarea class="form-control" id="txtalamatnasabah" asp-for="customer_address" rows="" placeholder="Input Alamat" required></textarea>

                                <p>Lokasi</p>
                                <div class="row">
                                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                        <input type="hidden" asp-for="customer_location_id" id="txlocationid" required />
                                        @{
                                            var locationCustomerName = "";
                                            if (isEdit)
                                            {
                                                var dataLocation = (MsLocationDetailModels)Commons.GetLoctionById((int)Model.customer_location_id);
                                                if (dataLocation != null)
                                                {
                                                    locationCustomerName = Commons.CombineLocationName(dataLocation.village_name, dataLocation.district_name, dataLocation.regency_name, dataLocation.province_name, dataLocation.zip_code);
                                                }
                                            }
                                        }
                                        <textarea class="form-control" disabled="" rows="" id="txlocationname" placeholder="Pilih Lokasi" required>@locationCustomerName</textarea>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        <button class="btn btn-warning btn-input ladda-button" data-style="zoom-in" type="button" id="btnshowlokasi" style="width: 100%; margin-bottom: 5%;">Pilih lokasi</button>

                                        <div id="mdllocation" class="modal">
                                            <!-- Modal content -->
                                            <div class="modal-content">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                        <h4>Pilih Lokasi</h4>
                                                        <div class="tile">
                                                            <div class="modal-body" id="containerLocation"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <div id="Perseorangan" style="display:none;">
                                    <p>Nama Nasabah</p>
                                    <input type="text" asp-for="customer_name" id="txnamanasabah_p" class="form-control type-perorang" placeholder="Input Nama Nasabah">

                                    <div class="row">
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                            <p>Tempat Lahir</p>
                                            <input type="text" id="txtempatlahir_p" asp-for="birth_place" class="form-control type-perorang" placeholder="Input Tempat Lahir">
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                            <p>Tanggal Lahir</p>
                                            <input class="form-control type-perorang" id="tanggallahir_p" asp-for="birth_date" type="text" placeholder="Input Tanggal Lahir">
                                        </div>
                                    </div>
                                </div>

                                <div id="Corporate1" style="display:none;">
                                    <p>Nama Perusahaan</p>
                                    <input type="text" id="txnamaperusahaan_c" class="form-control type-corporate" asp-for="company_name" placeholder="Input Nama Perusahaan">
                                </div>

                                <p>Jenis Kelamin</p>
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p><input class="form-check-input" value="L" type="radio" asp-for="gender" id="rbLaki">&nbsp;&nbsp;&nbsp;&nbsp;Laki - Laki</p>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p><input class="form-check-input" value="P" type="radio" asp-for="gender" id="rbPerempuan">&nbsp;&nbsp;&nbsp;&nbsp;Perempuan</p>
                                </div>

                                <p style="margin-top: 13%;">Nama Ibu Kandung</p>
                                <input type="text" asp-for="mother_name" id="txnamaibukandung" class="form-control" placeholder="Input Nama Ibu Kandung" required>

                            </div>
                            <!-- END KOLOM 1 -->
                            <!-- KOLOM 2 -->
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">

                                <div id="Perseorangan1" style="display:none;">
                                    <p style="margin-top: -1%;">Email</p>
                                    <input type="text" asp-for="email" id="txemail" class="form-control type-perorang" placeholder="Input Email">
                                </div>

                                <div id="Corporate2" style="display:none;">
                                    <p>Domisili Perusahaan</p>
                                    <input type="text" asp-for="birth_place" id="txdomisiliperusahaan" asp-for="company_birth_place" class="form-control type-corporate" placeholder="Input Domisili Perusahaan">

                                    <p>Tanggal Berdiri Perusahaan</p>
                                    <input class="form-control type-corporate" id="tanggallahirnasabah" asp-for="birth_date" type="text" placeholder="Input Tanggal Lahir">
                                </div>

                                <p>No NPWP</p>
                                <input type="number" pattern="/^-?\d+\.?\d*$/" onKeyPress="if(this.value.length==16) return false;" asp-for="npwp_no" id="txNPWP" class="form-control" placeholder="Input No NPWP">

                                <p>Telepon</p>
                                <input type="number" pattern="/^-?\d+\.?\d*$-/" onKeyPress="if(this.value.length==15) return false;" asp-for="telephone_number" id="txtelephone" class="form-control" placeholder="Input Telepon" required>

                                <p>Handphone</p>
                                <input type="number" pattern="/^-?\d+\.?\d*$/" onKeyPress="if(this.value.length==15) return false;" asp-for="mobile_phone" id="txMobilePhone" class="form-control" placeholder="Input Handphone" required>

                                <p>Jarak dari rumah ke kantor (KM)</p>
                                <input type="number" pattern="/^-?\d+\.?\d*$/" onKeyPress="if(this.value.length==5) return false;" asp-for="residence_distance" id="txResidenceDistance" class="form-control" placeholder="Input Jarak dari rumah ke kantor (KM)" required>

                                <p>Asal Konsumen</p>
                                <select class="form-control" id="ddlasalkonsumen" asp-for="customer_source_id" asp-items="@ViewBag.CustomerSource" required>
                                    <option value="">Pilih Asal Konsumen</option>
                                </select>

                                <p style="margin-top: 5%;">Sudah Pernah Survey</p>
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p><input class="form-check-input" type="radio" asp-for="is_surveyed" value="true" id="rbSurveyedYa">&nbsp;&nbsp;&nbsp;&nbsp;Sudah</p>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p><input class="form-check-input" type="radio" asp-for="is_surveyed" value="false" id="rbSurveyedNo">&nbsp;&nbsp;&nbsp;&nbsp;Belum</p>
                                </div>
                                <br /><br />
                                <div id="pnlBtnApuppt">
                                    <hr />
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-top: 20px;">
                                        <button class="btn btn-warning btn-input" id="btnShowApuppt" style="width: 100%; margin-bottom: 5%;">Cek APUPPT</button>
                                    </div>
                                </div>
                                <br>
                                @{
                                    var isDisplay = "none";
                                    if (isEdit)
                                    {
                                        if (Model.is_apuppt != 0)
                                        {
                                            isDisplay = "block";
                                        }
                                    }
                                }
                                <div id="pnlApuppt" style="display:@isDisplay;">
                                    <input type="hidden" asp-for="is_apuppt" id="hdn_isapuppt" value="0" />
                                    @if (isEdit)
                                    {
                                        var dataApuppt = (TrCasApupptModels)Model.Apuppt;
                                        if (Model.is_apuppt != 0)
                                        {
                                            <p style="margin-top: 9%;">Nama Orang Tua</p>
                                            <input type="hidden" asp-for="Apuppt.question_id" value="1" />
                                            <input type="text" asp-for="Apuppt.answer" value="@dataApuppt.answer[0]" id="ddlnamaorgtua" class="form-control" placeholder="Input Nama Orang Tua">

                                            <p>Kewarganegaraan</p>
                                            <input type="hidden" asp-for="Apuppt.question_id" value="2" />
                                            <select class="form-control" asp-items="@ViewBag.SelectNationality" asp-for="Apuppt.answer" id="ddlkewarganegaraan">
                                                <option>Pilih Kewarganegaraan</option>
                                            </select>

                                            <p>Pernah Keluar Negeri Atau Tidak</p>
                                            <input type="hidden" asp-for="Apuppt.question_id" value="3" />
                                            <select class="form-control" id="ddlvisiting" asp-for="Apuppt.answer">
                                                <option>Pilih</option>
                                                <option value="Y">Ya</option>
                                                <option value="N">Tidak</option>
                                            </select>
                                        }
                                    }
                                    else
                                    {
                                        <p style="margin-top: 9%;">Nama Orang Tua</p>
                                        <input type="hidden" asp-for="Apuppt.question_id" value="1" />
                                        <input type="text" asp-for="Apuppt.answer" id="ddlnamaorgtua" class="form-control" placeholder="Input Nama Orang Tua">

                                        <p>Kewarganegaraan</p>
                                        <input type="hidden" asp-for="Apuppt.question_id" value="2" />
                                        <select class="form-control" asp-items="@ViewBag.SelectNationality" name="Apuppt.answer" id="ddlkewarganegaraan">
                                            <option>Pilih Kewarganegaraan</option>
                                        </select>

                                        <p>Pernah Keluar Negeri Atau Tidak</p>
                                        <input type="hidden" asp-for="Apuppt.question_id" value="3" />
                                        <select class="form-control" id="ddlvisiting" name="Apuppt.answer">
                                            <option>Pilih</option>
                                            <option value="Y">Ya</option>
                                            <option value="N">Tidak</option>
                                        </select>
                                    }

                                </div>
                            </div>
                            <!-- END KOLOM 2 -->

                        </div>
                    </div>
                </Identitas>
            </div>
        </div>
        <!-- END PERORANGAN -->
        <!-- BADAN USAHA -->
        <div class="container" id="Corporate" style="display: none;">
            <button id="BadanUsaha_more" class="BadanUsaha_more" type="button" style="width: 100%;">Badan Usaha</button>
            <div class="data-input">
                <btnBadanUsaha><h4><div class="bgc-menu">Badan Usaha <i class="fa fa-chevron-circle-down" aria-hidden="true"></i></div></h4></btnBadanUsaha>
                <BadanUsaha class="BadanUsaha" id="BadanUsaha">
                    <div class="tile">
                        <div class="form-group row">

                            <!-- KOLOM 1 -->
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="border-right: 2px solid #e8e8e8">

                                <div class="bgc-small">Data Corporate</div>

                                <p>Nama Komisaris</p>
                                <input type="text" class="form-control type-corporate" id="txnamakomisaris" asp-for="CorporateDocuments.corporate_commisioner_name" placeholder="Input Nama Komisaris">

                                <p>Nama Direktur</p>
                                <input type="text" class="form-control type-corporate" id="txnamadirektur" asp-for="CorporateDocuments.corporate_director_name" placeholder="Input Nama Direktur">

                                <p>Status Perusahaan</p>
                                <input type="text" class="form-control type-corporate" id="txstatusperusahaan" asp-for="CorporateDocuments.corporate_status" placeholder="Input Status Perusahaan">

                                <p>Bidang Usaha Perusahaan</p>
                                @if (isEdit)
                                {
                                    if (Model.CorporateDocuments != null)
                                    {
                                        var dtBidangUsahaList = (List<MsIndustryTypeModels>)ViewBag.SelectIndustryType.Items;
                                        var industryTypeSelected = Model.CorporateDocuments.corporate_industry_id;
                                        <select class="form-control type-corporate" id="ddlbidangusaha_c" asp-for="CorporateDocuments.corporate_industry_id" onchange="cas_load.OtherIndustryDesc();">
                                            <option value="">Pilih Bidang Usaha Perusahaan</option>
                                            @{
                                                foreach (var item in dtBidangUsahaList.ToList())
                                                {
                                                    if (item.industry_type_id == Convert.ToInt32(industryTypeSelected))
                                                    {
                                                        <option value="@item.industry_type_id" selected>@item.industry_type_desc</option>
                                                    }
                                                    else
                                                    {
                                                        <option value="@item.industry_type_id">@item.industry_type_desc</option>
                                                    }
                                                }
                                            }
                                        </select>
                                    }
                                    else
                                    {
                                        <select class="form-control type-corporate" id="ddlbidangusaha_c" asp-items="@ViewBag.SelectIndustryType" asp-for="CorporateDocuments.corporate_industry_id" onchange="cas_load.OtherIndustryDesc();">
                                            <option value="">Pilih Bidang Usaha Perusahaan</option>
                                        </select>
                                    }
                                }
                                else
                                {
                                    <select class="form-control type-corporate" id="ddlbidangusaha_c" asp-items="@ViewBag.SelectIndustryType" asp-for="CorporateDocuments.corporate_industry_id" onchange="cas_load.OtherIndustryDesc();">
                                        <option value="">Pilih Bidang Usaha Perusahaan</option>
                                    </select>
                                }

                                <div id="pnlOtherIndustry" style="display: none;">
                                    <p>Bidang Usaha Lainnya</p>
                                    <input type="text" class="form-control" id="txotherindustrydesc" asp-for="CorporateDocuments.corporate_other_industry" placeholder="Input Bidang Usaha Lainnya">
                                </div>

                                <p>Lama Usaha</p>
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <input type="text" class="form-control type-corporate" id="txlamatahunusaha" asp-for="CorporateDocuments.corporate_year_period" placeholder="Tahun">
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <input type="text" class="form-control type-corporate" id="txlamabulanusaha" asp-for="CorporateDocuments.corporate_month_period" placeholder="Bulan">
                                    </div>
                                </div>

                                <div class="row" style="margin-top: 3%;">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <p>Jumlah Karyawan</p>
                                        <input type="number" class="form-control type-corporate" id="txjumlahkaryawan" asp-for="CorporateDocuments.corporate_number_of_employee" placeholder="Orang">
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <p>Gol Debitur</p>
                                        <input type="text" class="form-control type-corporate" asp-for="CorporateDocuments.corporate_debitur_type" placeholder="Gol Debitur">
                                    </div>
                                </div>

                                <div class="row" style="margin-top: 3%;">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <p>Telepon Perusahaan</p>
                                        <input type="number" class="form-control type-corporate" asp-for="CorporateDocuments.corporate_telephone_number" placeholder="Input Tlp Perusahaan">
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <p>Fax Perusahaan</p>
                                        <input type="number" class="form-control" asp-for="CorporateDocuments.corporate_fax_number" placeholder="Input Fax Perusahaan">
                                    </div>
                                </div>

                                <p style="margin-top: 3%;">Situs</p>
                                <input type="text" class="form-control type-corporate" placeholder="Input Situs" asp-for="CorporateDocuments.corporate_site">

                                <p>Email Perusahaan</p>
                                <input type="text" class="form-control type-corporate" placeholder="Input Email Perusahaan" asp-for="CorporateDocuments.corporate_email">

                                <div class="bgc-small">Akta Pendirian Perusahaan</div>
                                <p>No Akta</p>
                                <input type="text" class="form-control type-corporate" placeholder="Input No Akta" asp-for="CorporateDocuments.founders_deed_number">

                                <p>Tanggal Akta</p>
                                <input class="form-control type-corporate" id="tanggalaktaperusahaan" type="text" asp-for="CorporateDocuments.founders_deed_date" placeholder="Input Tanggal Akta">

                                <p>Nama Notaris</p>
                                <input type="text" class="form-control type-corporate" asp-for="CorporateDocuments.founders_notary_name" placeholder="Input Nama Notaris">

                            </div>
                            <!-- END KOLOM 1 -->
