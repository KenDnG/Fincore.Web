                    <div class="tile">
                        <div class="form-group row">

                            <!-- KOLOM 1 -->
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="border-right: 2px solid #e8e8e8">

                                <p>Biaya Pengeluaran Rumah Tangga (/Bulan)</p>
                                <input class="form-control" id="txtbiayapengeluaranRT" type="number" asp-for="Financials.household_expenses" placeholder="Rp">

                                <p>Biaya Pendidikan (/Bulan)</p>
                                <input class="form-control" id="txbiayapendidikan" type="number" asp-for="Financials.education_expenses" placeholder="Rp">

                                <p>Biaya Kesehatan (/Bulan)</p>
                                <input class="form-control" id="txbiayakesehatan" type="number" placeholder="Rp" asp-for="Financials.health_expenses">

                            </div>
                            <!-- END KOLOM 1 -->
                            <!-- KOLOM 2 -->
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">

                                <p>Jumlah Tanggungan</p>
                                <input class="form-control" id="txjumlahtanggungan" type="number" asp-for="Financials.number_of_dependents" placeholder="Input Jumlah Tanggungan" required>

                                <p style="margin-top: 8%;">Cicilan</p>
                                @{
                                    var mInstallment = ViewBag.OtherInstallments;
                                }
                                @if (mInstallment != null)
                                {
                                    @for (var i = 0; i < @mInstallment.Count; i++)
                                    {
                                        var isChecked = "";
                                        if (isEdit)
                                        {
                                            var dataInstallment = Model.Installments;
                                            if (dataInstallment != null)
                                            {
                                                try
                                                {
                                                    //handle if array out of ranges
                                                    //note dev: need to improve for this condition
                                                    if (dataInstallment.monthly_other_installment_id.Length <= 1)
                                                    {
                                                        isChecked = dataInstallment.monthly_other_installment_id[0] == @mInstallment[i].monthly_other_installment_id ? "checked" : "";
                                                    }
                                                    else
                                                    {
                                                        isChecked = dataInstallment.monthly_other_installment_id[i] == @mInstallment[i].monthly_other_installment_id ? "checked" : "";
                                                    }
                                                }
                                                catch (Exception ex)
                                                {
                                                    isChecked = "";
                                                }
                                            }

                                            var ids = $"chk{@mInstallment[i].monthly_other_installment_desc}";
                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <input type="checkbox" value="@mInstallment[i].monthly_other_installment_id" id="@ids.Replace(" ","")" name="Installments.monthly_other_installment_id" @isChecked><span class="label-text"> @mInstallment[i].monthly_other_installment_desc </span>
                                            </div>
                                        }
                                        else
                                        {
                                            var ids = $"chk{@mInstallment[i].monthly_other_installment_desc}";
                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <input type="checkbox" value="@mInstallment[i].monthly_other_installment_id" id="@ids.Replace(" ","")" name="Installments.monthly_other_installment_id"><span class="label-text"> @mInstallment[i].monthly_other_installment_desc </span>
                                            </div>
