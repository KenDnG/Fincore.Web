                                                                {
                                                                    <option value="@djt.JobTitleId" selected>@djt.JobTitleDescription</option>
                                                                }
                                                                else
                                                                {
                                                                    <option value="@djt.JobTitleId">@djt.JobTitleDescription</option>
                                                                }
                                                            }
