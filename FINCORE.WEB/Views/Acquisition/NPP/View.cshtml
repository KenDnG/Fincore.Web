<!-- BUTTON CANCEL SAVE -->
<div class="container" style="margin-top: 5%; margin-bottom: 5%;">
    <div class="title text-center">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            @if(Model.ActionTitle == "APPROVAL" && Model.NPP.IsApprover == "1")
            {
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <a href="@Url.Action("Inbox","Approval")" class="btn-submit" id="btnCancel" style="width: 100%;background-color: red;">Back</a>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <button type="button" class="btn-submit" id="btnReview" style="width: 100%;">Review</button>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <button type="button" class="btn-submit" id="btnReject" style="width: 100%;">Reject</button>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <button type="button" class="btn-submit" id="btnApprove" style="width: 100%;">Approve</button>
                </div>
            }
            else
            {
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <a href="@Url.Action("Index","NPP")" class="btn-submit" id="btnCancel" style="width: 100%;background-color: red;">Back</a>
                </div>
            }
        </div> 
    </div>
</div>
