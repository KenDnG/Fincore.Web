@{
    ViewData["Title"] = "NPP - " + @Model.ActionTitle;
}

<!-- CM PO ADD -->
<div class="container">
    <div id="contactform">
        <div class="data-input"><btnCMPO><h4><div class="bgc-menu">View NPP <i class="fa fa-chevron-circle-down" aria-hidden="true"></i></div></h4></btnCMPO>
            <CMPO class="CMPO" id="CMPO">

                <div class="tile">
                    <div class="form-group row">

                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-bottom: 10px">
                            <div class="text-left">
                                <table id="example" class="table table-striped table-bordered" style="font-size: 12px; text-align:center;">
                                    <thead>
                                        <tr>
                                            <th>
                                                <p>Credit Id</p>
                                                <input id="CreditId" value="@Model.NPP.CreditId" type="text" class="form-control" style="margin-bottom: 0;" readonly>
                                            </th>
                                            <th>
                                                <p>No NPP Passnet</p>
                                                <input id="AgreementOld" value="@Model.NPP.AgreementOld" type="text" class="form-control" style="margin-bottom: 0;" readonly>
                                            </th>
                                            <th>
                                                <p>Agreement Date</p>
                                                <input id="AgreementDate" value="@Model.NPP.AgreementDate" type="text" class="form-control" style="margin-bottom: 0;" readonly>
                                            </th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>

                        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                
                            <p>Tanggal CM</p>
                            <input id="CMDate" value="@Model.NPP.CreditDate" class="form-control" readonly>
                                
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                
                            <p>PO No</p>
                            <input id="PONumber" value="@Model.NPP.PONumber" class="form-control" readonly>

                        </div>

                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                
                            <p>Nama Konsumen</p>
                            <input id="ConstomerName" value="@Model.NPP.CustomerName" class="form-control" readonly>

                        </div>

                        <!-- END KOLOM 1 -->

                    </div>
                </div>

            </CMPO>
        </div>
    </div>
</div>
<!-- END CM PO ADD -->

